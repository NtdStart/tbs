<div id="delay" >
        <table class="table" id="tab">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">Mã vận đơn</th>
                    <th scope="col">Trạng thái</th>
                    <th scope="col">Lý do</th>
                    <th scope="col">Tiền thu hộ</th>
                    <th scope="col">Thông tin xử lý</th>
                </tr>
            </thead>    

        <tbody id="tbody<%=unhandle.label_id%>">

        <td><%= unhandle.label_id %>
        <td><%= unhandle.status_info %></td>
            <td style='color:red'>
        <% unhandle.arrJson.forEach((e, i) => { %>
            <span style='color:blue'>Lần  <%= ++i %> : </span><span style='color:red'>  <%= e.reason %>  </span><span  style='color:blue'> ( <%=e.action_time%> )<span><br />
        <% }) %>
        </td>

        <td style='color:red'><span style='color:blue'>Lần  1: </span><span style='color:red'> <%= unhandle.arrJson[0].pick_money%> </span></td>
            <td>    
                            <div id="layhand<%=unhandle.label_id%>" ></div> 
                            <input type="text" name="message" id="layhandling<%=unhandle.label_id%>">                   
                            <button class="btn btn-success" onclick="saveMessageLay('<%=unhandle.label_id%>')" >Save</button>
            </td>
        </tbody>
    </table>
</div id="">